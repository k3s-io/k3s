{
  "Vendor": true,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "Exclude": [
    ".*\\.pb\\.go",
    "fetch\\.go:.*::error: unrecognized printf verb 'r'"
  ],
  "EnableGC": true,

  "Enable": [
    "structcheck",
    "varcheck",
    "staticcheck",
    "unconvert",

    "gofmt",
    "goimports",
    "golint",
    "ineffassign",
    "vet"
  ]
}
